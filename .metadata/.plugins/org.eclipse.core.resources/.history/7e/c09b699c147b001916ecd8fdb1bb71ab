
public class Runner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		print1To100();
		print100To999();
	}

	private static void print1To100() {
		for (int i = 1; i < 11; i++) {
			System.out.println(i);
		}
	}

	private static void print100To999() {
		for (int i = 100; i < 1000; i++) {
			System.out.println(i);
		}
	}

}
