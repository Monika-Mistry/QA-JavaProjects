
public class Calculator {

	private int numberA;
	private int numberB;

	public Calculator(int numA, int numB) {
		this.numberA = numA;
		this.numberB = numB;
	}

	public int addTwoNumbers() {
		return numberA + numberB;
	}

	public int subtractTwoNumbers() {
		return numberA - numberB;
	}

	public int divideTwoNumbers() {
		return numberA / numberB;
	}

	public int multiplyTwoNumbers() {
		return numberA * numberB;
	}
}
