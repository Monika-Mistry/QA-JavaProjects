package librarymodel;

public class City extends Map {

	private String city;

	public City(int id, String title, Status status, int yearPublished, int scale, String country, String city) {
		super(id, title, status, yearPublished, scale, country);
		this.city = city;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

}
