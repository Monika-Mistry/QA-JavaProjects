package librarymodel;

import java.util.ArrayList;

public class NonFiction extends Book {

	private String category;
	private int edition;

	public NonFiction(int id, String title, Status status, int yearPublished, String isbn, int pages,
			ArrayList<String> authors, String category, int edition) {
		super(id, title, status, yearPublished, isbn, pages, authors);
		this.category = category;
		this.edition = edition;
	}

}
