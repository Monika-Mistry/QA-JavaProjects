
public class Person {

	String name;
	int age;
	String jobTitle;
	
	public Person() {
		this.name = name;
		this.age = age;
		this.jobTitle = jobTitle;
	}

	public String getName() {
		return name;
	}

	public int getAge() {
		return age;
	}

	public String getJobTitle() {
		return jobTitle;
	}

	public String getPerson() {
		return "Name: " + name + " Age: " + age + " Job Title: " + jobTitle;
	}

}
